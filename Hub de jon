local Buttonevade3 = Evade:CreateButton({
   Name = "DD Gui,Speed",
   Info = "Evade", -- Información opcional.
   Interact = 'Button',
   Callback = function()
      local key = "s?zpk9bi3E" -- Key que quieres copiar

      -- Intenta copiar la key al portapapeles si el exploit lo soporta
      if setclipboard then
         setclipboard(key)
         Rayfield:Notify({
            Title = "Key Copiada",
            Content = "La Key ha sido copiada a tu portapapeles.",
            Duration = 6.5,
            Image = 4483362458,
            Actions = {
               Ignore = {
                  Name = "Okay!",
                  Callback = function()
                     print("El usuario presionó Okay!")
                  end
               },
            },
         })
      else
         Rayfield:Notify({
            Title = "Error",
            Content = "Tu exploit no soporta copiar al portapapeles.",
            Duration = 6.5,
            Image = 4483362458,
            Actions = {
               Ignore = {
                  Name = "Entendido",
                  Callback = function()
                     print("El usuario no puede copiar la key automáticamente.")
                  end
               },
            },
         })
      end

      -- Carga el script remoto
      loadstring(game:HttpGet("https://raw.githubusercontent.com/ddario66/Test2/refs/heads/main/Evade"))()
   end,
})



